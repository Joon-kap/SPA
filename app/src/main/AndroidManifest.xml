<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cmu.ajou.spa">

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET" />

    <android:uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <android:uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WebSocketActivity" />
        <activity android:name=".HttpConnection" />
        <activity android:name=".Confirm_reservation" />
        <activity android:name=".payment_process" />
        <activity android:name=".Parking_process" />
        <activity android:name=".SelectCardActivity" />
        <activity android:name=".CardNumberActivity" />
        <activity android:name=".CardInformationActivity" />
        <activity android:name=".OpenEnterGate" />
        <activity android:name=".OpenExitGate" />
        <activity android:name=".FinishActivity"></activity>
    </application>

</manifest>